<odoo>
    <data>
        <record id="demo_model_view_tree" model="ir.ui.view">
            <field name="name">demo.model.tree</field>
            <field name="model">demo.model</field>
            <field name="arch" type="xml">
                <tree string="Demo objects">
                    <field name="name"/>
                    <field name="price" string="Price"/>
                    <field name="currency_id" invisible="true"/>
                    <field name="quantity" string="Quantity, kg" sum="Sum"/>
                    <field name="total_bill" string="Total bill" sum="Sum"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>